{
    "name": "cppshowcase",
    "version": "1.1.0",
    "description": "Showcase app of my C++ work for my IB personal project",
    "main": "src/js/main.js",
    "engines": {
        "pnpm": ""
    },
    "scripts": {
        "start": "electron src/js/main.js",
        "build": "electron-builder build --win --x64"
    },
    "build": {
        "win": {
            "target": "squirrel"
        },
        "productName": "C++ Showcase",
        "icon": "src/media/img/CppIcon.ico",
        "loadingGif": "src/media/img/splash.png"
    },
    "author": "shob3r",
    "license": "MIT",
    "devDependencies": {
        "electron": "^23.1.0",
        "electron-builder": "24.0.0-alpha.12",
        "electron-builder-squirrel-windows": "24.0.0-alpha.12"
    },
    "dependencies": {
        "electron-updater": "^5.3.0"
    }
}
